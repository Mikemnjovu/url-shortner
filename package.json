  "author": "mikenjovu <mikemnjovu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@innocells/branch-naming-check": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "morgan": "^1.10.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "nodemon server.js",
    "test": "jest",
    "dev-test": "jest --watchAll"
